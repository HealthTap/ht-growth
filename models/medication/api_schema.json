                                    "text_type": {
                                        "enum": ["ordered_list", "unordered_list"]
                                    },
                                    "value": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }, {
                                "type": "object",
                                "required": ["text_type", "value"],
                                "properties": {
                                    "text_type": {
                                        "enum": ["paragraph"]
                                    },
                                    "value": {
                                        "type": "string"
                                    }
                                }
                            }]
                        }
                    }
                }
            }
        }
